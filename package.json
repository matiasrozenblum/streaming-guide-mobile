{
  "name": "streaming-guide-mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "npx expo start",
    "start:dev": "APP_VARIANT=development npx expo start",
    "start:staging": "APP_VARIANT=staging npx expo start",
    "start:prod": "APP_VARIANT=production npx expo start",
    "android": "npx expo run:android",
    "android:staging": "APP_VARIANT=staging npx expo run:android",
    "android:prod": "APP_VARIANT=production npx expo run:android",
    "ios": "npx expo run:ios",
    "ios:staging": "APP_VARIANT=staging npx expo run:ios",
    "ios:prod": "APP_VARIANT=production npx expo run:ios",
    "web": "npx expo start --web",
    "build:android": "npx expo prebuild --platform android --clean && cd android && ./gradlew assembleRelease",
    "build:android:staging": "APP_VARIANT=staging npx expo prebuild --platform android --clean && cd android && APP_VARIANT=staging ./gradlew assembleRelease",
    "build:android:prod": "APP_VARIANT=production npx expo prebuild --platform android --clean && cd android && APP_VARIANT=production ./gradlew assembleRelease",
    "build:ios": "expo run:ios --configuration Release --device",
    "build:ios:sim": "expo run:ios --configuration Release"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.2",
    "@expo-google-fonts/outfit": "^0.4.3",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-firebase/app": "^23.8.6",
    "@react-native-firebase/messaging": "^23.8.6",
    "@react-navigation/bottom-tabs": "^7.12.0",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.12.0",
    "@react-navigation/stack": "^7.7.1",
    "@reduxjs/toolkit": "^2.11.2",
    "@shopify/flash-list": "^2.2.2",
    "axios": "^1.13.4",
    "dayjs": "^1.11.19",
    "expo": "~54.0.33",
    "expo-apple-authentication": "~8.0.8",
    "expo-application": "^7.0.8",
    "expo-build-properties": "^1.0.10",
    "expo-constants": "^18.0.13",
    "expo-crypto": "~15.0.8",
    "expo-device": "^8.0.10",
    "expo-file-system": "^19.0.21",
    "expo-font": "~14.0.11",
    "expo-linear-gradient": "~15.0.8",
    "expo-localization": "^17.0.8",
    "expo-notifications": "~0.32.16",
    "expo-secure-store": "^15.0.8",
    "expo-status-bar": "~3.0.9",
    "posthog-react-native": "^4.30.2",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "^2.30.0",
    "react-native-pager-view": "6.9.1",
    "react-native-paper": "^5.15.0",
    "react-native-reanimated": "^4.2.1",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.23.0",
    "react-native-svg": "15.12.1",
    "react-native-webview": "^13.16.0",
    "react-native-youtube-iframe": "^2.4.1",
    "react-redux": "^9.2.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@expo/config-plugins": "^54.0.4",
    "@types/react": "~19.1.0",
    "babel-preset-expo": "^54.0.10",
    "typescript": "~5.9.2"
  },
  "private": true
}
